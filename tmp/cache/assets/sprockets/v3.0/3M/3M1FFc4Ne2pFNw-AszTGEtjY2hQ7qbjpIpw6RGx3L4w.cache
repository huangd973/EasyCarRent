I"û(function() {
  var desktop_ui_init, mobile_ui_init;

  desktop_ui_init = function() {
    return jQuery(document).on('turbolinks:load', function() {});
  };

  $('.scroller').slick({
    lazyLoad: 'ondemand',
    slidesToShow: 3,
    slidesToScroll: 1,
    arrows: true
  });

  mobile_ui_init = function() {
    return jQuery(document).on('turbolinks:load', function() {});
  };

  $('.scroller').slick({
    dots: true,
    arrows: true
  });

  $(function() {
    if ($(window).width() < 620) {
      mobile_ui_init();
    } else {
      desktop_ui_init();
    }
    return $(window).resize(function() {
      if ($(this).width() < 620) {
        return mobile_ui_init();
      } else {
        return desktop_ui_init();
      }
    });
  });

}).call(this);
