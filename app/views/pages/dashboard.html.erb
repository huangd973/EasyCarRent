<div class="row">
  <div class="col-sm-2">
  <div style='width: 250px;'>
    <!-- <div id="sidebar_container">Liste des vehicules</div> -->
  </div>
  </div>
  <div class="col-sm-10">
    <div id="sidebar_builder" style='width: cover; height: 500px;'></div>
  </div>
</div>
<div class="row">
  <% @cars_all.each do |car| %>
  <div class="col-sm-2">
      <p id="sidebar_container" style="font-weight: bold"></p>
      <p><%= car.address %></p>
      <p><%= car.odometer %> km</p>
      <p><%= car.fueltank %> L car Tank</p>
      <p><%= car.computerfueltank %> L car_computer Tank</p>
  </div>
  <% end %>
</div>
<h2 class="text-center">Location en cours </h2>

<% Rental.all.each do |rental| %>
<div class="message">
  <img class="avatar-large" src="https://kitt.lewagon.com/placeholder/users/gabriel-dehan">
  <div class="message-name">
    <h3><%= User.find(rental.user_id).email %></h3>
    <p><%= rental.created_at %></p>
  </div>
  <div class="message-body">
    <p><strong><%= Car.find(rental.car_id).name %></strong></p>
    <p><%= rental.starts_on%></p>
    <p><%= rental.ends_on %></p>
    <p><%= Car.find(rental.car_id).plaque %></p>
  </div>
  <div class="message-status">
    <p class="status pending">Pending</p>
    <p class="price">â‚¬345</p>
  </div>
</div>
<% end %>
