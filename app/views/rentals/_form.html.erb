
<%= simple_form_for([@car, @rental]) do |f| %>
<% if current_user.try(:admin?) %>
     <%= f.input :status, :collection => [['Available','available'],['Offline','offline']]  %>
<% end %>
<% if current_user.try(:admin!) %>
     <%= f.input :starts_on %>
     <%= f.input :ends_on %>
     <%= f.submit nil, class: 'btn btn-success' %>
<% end %>
<% end %>
